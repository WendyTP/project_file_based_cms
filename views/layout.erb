<html>
<head>
  <title>Cms App</title>
  <link rel="stylesheet" href="/stylesheets/cms.css" type="text/css">
</head>
<body>
  <% if session[:error] %>
    <div class="flash error">
      <p><%= session.delete(:error) %></p>
    </div>
  <% end %>

  <% if session[:success] %>
    <div class="flash success">
      <p><%= session.delete(:success) %></p>
    </div>
  <% end %>
  <main>
  <%= yield %>
  </main>
</body>
</html>